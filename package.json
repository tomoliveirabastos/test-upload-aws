{
  "name": "upload-file-test-aws",
  "version": "1.0.0",
  "description": "File Upload & Metadata Extraction Service with AWS S3, Lambda, and DynamoDB",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "lambda:build": "webpack --config webpack.config.js",
    "deploy:lambda": "npm run lambda:build && aws lambda update-function-code --function-name file-metadata-extractor --zip-file fileb://dist/lambda.zip"
  },
  "keywords": ["aws", "s3", "lambda", "dynamodb", "file-upload", "metadata", "typescript", "nodejs"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.650.0",
    "@aws-sdk/client-dynamodb": "^3.650.0",
    "@aws-sdk/client-lambda": "^3.650.0",
    "@aws-sdk/lib-dynamodb": "^3.650.0",
    "@aws-sdk/s3-request-presigner": "^3.650.0",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.33.0",
    "mime-types": "^2.1.35",
    "compression": "^1.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/pdf-parse": "^1.1.4",
    "@types/mime-types": "^2.1.4",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "rimraf": "^5.0.5",
    "tsconfig-paths": "^4.2.0",
    "@types/aws-lambda": "^8.10.130"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
